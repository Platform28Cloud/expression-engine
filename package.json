    "url": "https://github.com/plantain-00/expression-engine/issues"
  },
  "homepage": "https://github.com/plantain-00/expression-engine#readme",
  "devDependencies": {
    "@babel/parser": "7.10.4",
    "@microsoft/api-extractor": "7.9.1",
    "@rollup/plugin-node-resolve": "8.1.0",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "ava": "3.9.0",
    "clean-scripts": "1.16.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-plantain": "1.0.0",
    "markdownlint-cli": "0.23.2",
    "no-unused-export": "1.11.0",
    "pegjs": "^0.10.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rimraf": "3.0.2",
    "rollup": "2.18.2",
    "rollup-plugin-uglify": "6.0.4",
    "select2-react-component": "5.10.2",
    "ts-node": "8.10.2",
    "type-coverage": "2.8.3",
    "typescript": "3.9.6",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  },
  "module": "browser/index.js",
  "unpkg": "expression-engine.min.js",
  "jsdelivr": "expression-engine.min.js",
  "typeCoverage": {
    "atLeast": 99.55

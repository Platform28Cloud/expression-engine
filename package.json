{
  "name": "expression-engine",
  "version": "1.8.1",
  "description": "An expression tokenizer, parser and evaluator.",
  "main": "nodejs/index.js",
  "scripts": {
    "build": "clean-scripts build",
    "lint": "clean-scripts lint",
    "test": "clean-scripts test",
    "fix": "clean-scripts fix",
    "pegjs": "clean-scripts build[1].front[2]",
    "release": "clean-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/expression-engine.git"
  },
  "keywords": [
    "expression",
    "tokenizer",
    "parser",
    "evaluator"
  ],
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/expression-engine/issues"
  },
  "homepage": "https://github.com/plantain-00/expression-engine#readme",
  "devDependencies": {
    "@babel/parser": "7.12.17",
    "@microsoft/api-extractor": "7.13.1",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "ava": "3.15.0",
    "clean-release": "2.15.2",
    "clean-scripts": "1.20.1",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-plantain": "1.1.0",
    "markdownlint-cli": "0.26.0",
    "no-unused-export": "1.14.1",
    "pegjs": "^0.10.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.39.0",
    "rollup-plugin-uglify": "6.0.4",
    "select2-react-component": "6.1.0",
    "ts-node": "9.1.1",
    "tsconfig-plantain": "0.0.1",
    "type-coverage": "2.15.1",
    "typescript": "4.1.5",
    "webpack": "5.23.0",
    "webpack-cli": "4.5.0"
  },
  "module": "browser/index.js",
  "unpkg": "expression-engine.min.js",
  "jsdelivr": "expression-engine.min.js",
  "typeCoverage": {
    "atLeast": 99.55
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/utils.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  },
  "dependencies": {
    "tslib": "1 || 2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}

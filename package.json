    "ts-node": "8.10.2",
    "type-coverage": "2.7.1",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "module": "browser/index.js",
  "unpkg": "expression-engine.min.js",
  "jsdelivr": "expression-engine.min.js",
